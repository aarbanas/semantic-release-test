{
    "version": {
        "scheme": "SEMVER"
    },
    "commitMessageConventionsEnabled": "conventionalCommits",
    "services": {
        "github": {
            "type": "GITHUB",
            "options": {
                "AUTHENTICATION_TOKEN": "{{#environmentVariable}}GH_TOKEN{{/environmentVariable}}",
                "REPOSITORY_NAME": "{{#environmentVariable}}GITHUB_REPOSITORY_NAME{{/environmentVariable}}",
                "REPOSITORY_OWNER": "{{#environmentVariable}}GITHUB_REPOSITORY_OWNER{{/environmentVariable}}"
            }
        }
    }
}